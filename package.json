{
  "name": "vendo",
  "version": "1.3.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --no-clean",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "deploy": "cd dist && git add . && git commit -m \"v$npm_package_version\" && git push",
    "postbuild": "cp dist/index.html dist/404.html",
    "prebuild": "rimraf 'dist/**/!(.git)'",
    "predeploy": "npm run build"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "axios-mock-adapter": "^1.18.1",
    "bcryptjs": "^2.4.3",
    "core-js": "^3.6.5",
    "jsonwebtoken": "^8.5.1",
    "tailwindcss": "^1.4.6",
    "vue": "^2.6.11",
    "vue-router": "^3.2.0",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.4.5",
    "@vue/cli-plugin-eslint": "^4.4.5",
    "@vue/cli-plugin-router": "^4.4.5",
    "@vue/cli-plugin-unit-jest": "^4.4.5",
    "@vue/cli-plugin-vuex": "^4.4.5",
    "@vue/cli-service": "^4.4.5",
    "@vue/test-utils": "^1.0.3",
    "babel-eslint": "^10.1.0",
    "cypress": "^4.8.0",
    "eslint": "^6.7.2",
    "eslint-plugin-cypress": "^2.11.1",
    "eslint-plugin-vue": "^6.2.2",
    "rimraf": "^3.0.2",
    "vue-cli-plugin-axios": "0.0.4",
    "vue-cli-plugin-webpack-bundle-analyzer": "~2.0.0",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {},
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
